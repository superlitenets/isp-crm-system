attr_filter attr_filter.post-proxy {
    key = "%{Realm}"
    filename = ${modconfdir}/attr_filter/post-proxy
}

attr_filter attr_filter.pre-proxy {
    key = "%{Realm}"
    filename = ${modconfdir}/attr_filter/pre-proxy
}

