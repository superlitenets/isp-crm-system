# Database Configuration
POSTGRES_DB=license_db
POSTGRES_USER=license
POSTGRES_PASSWORD=your-secure-database-password

# License Server Configuration
LICENSE_PORT=8081
LICENSE_JWT_SECRET=generate-a-long-random-string-here
LICENSE_ADMIN_PASSWORD=YourSecureAdminPassword

# M-Pesa Configuration (for subscription payments)
MPESA_CONSUMER_KEY=your-consumer-key
MPESA_CONSUMER_SECRET=your-consumer-secret
MPESA_SHORTCODE=your-paybill-number
MPESA_PASSKEY=your-passkey
MPESA_ENV=production

# Public URL (for M-Pesa callbacks)
LICENSE_SERVER_URL=https://license.yourdomain.com

# Optional: Nginx HTTPS (use with --profile production)
HTTP_PORT=80
HTTPS_PORT=443
